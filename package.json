{
  "name": "HTC-sous-dev",
  "version": "1.0.0",
  "description": "Helper App for developing at HTC",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . HTC-sous-dev --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=HTC --version-string.FileDescription=HTC --version-string.ProductName=\"HTC-sous-dev\"",
    "package-linux": "electron-packager . HTC-sous-dev --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": "https://github.com/NYHTC/htc-sous-dev.git",
  "keywords": [
    "HTC",
    "sous-dev"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-packager": "^13.1.1"
  },
  "dependencies": {
    "applescript": "^1.0.0",
    "bootstrap": "^4.3.1",
    "jquery": "^3.3.1",
    "node-osascript": "^2.1.0",
    "popper.js": "^1.14.7"
  }
}
